{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "href": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "category"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.metadata"
    }
  }
}
